# Aliases

alias cat='bat'
alias ..='cd ..'
# some more ls aliases
alias ls='eza -l --git'
alias lst='eza --tree --level=2 -l --git'
alias lt="eza --tree --level=2 --long --icons --git"
alias ltree="eza --tree --level=2  --icons --git"

alias l='ls'
alias ll='ls -alFh'
alias la='ls -A'
alias j='z'

alias ibrew="arch -x86_64 /usr/local/bin/brew"

alias ovim='/usr/bin/vim'
alias vim='nvim'

